## Preferences

# http://stackoverflow.com/a/8660564/319266
ServerAliveInterval 30

## Wikimedia Labs
## https://wikitech.wikimedia.org/wiki/Help:Access#ProxyCommand

Host bastion.wmflabs.org tools-login.wmflabs.org
    User krinkle
    IdentityFile ~/.ssh/id_rsa_krinkle2
    ProxyCommand none
    ControlMaster auto

Host *.eqiad.wmflabs
    ProxyCommand ssh -a -W %h:%p bastion.wmflabs.org

## Wikimedia Production
## https://wikitech.wikimedia.org/wiki/Managing_Multiple_SSH_Agents

Host fenari.wikimedia.org bast1001.wikimedia.org iron.wikimedia.org bast4001.wikimedia.org hooft.esams.wikimedia.org
    User krinkle
    IdentityFile ~/.ssh/id_rsa_wm
    ProxyCommand none
    ControlMaster auto

Host gerrit.wikimedia.org
    ProxyCommand none

Host *.wikimedia.org
    ProxyCommand ssh -a -W %h:%p bast1001.wikimedia.org

Host *.pmtpa.wmnet
    ProxyCommand ssh -a -W %h:%p bast1001.wikimedia.org

Host *.eqiad.wmnet
    ProxyCommand ssh -a -W %h:%p bast1001.wikimedia.org

Host *.ulsfo.wmnet
    ProxyCommand ssh -a -W %h:%p bast4001.wikimedia.org

Host *.esams.wmnet
    ProxyCommand ssh -a -W %h:%p hooft.esams.wikimedia.org

## jQuery

Host *.jquery.com
    User krinkle
    IdentityFile ~/.ssh/id_rsa_jq
    # rmate
    RemoteForward 52698 localhost:52698
